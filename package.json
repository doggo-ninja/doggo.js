{
    "version": "0.0.4",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "description": "Typed client for Doggo.Ninja's public API",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "dev": "tsdx watch",
        "build": "tsdx build",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "peerDependencies": {},
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": false,
        "singleQuote": true,
        "trailingComma": "none",
        "useTabs": true
    },
    "name": "doggo.js",
    "author": "Kognise",
    "module": "dist/doggo.js.esm.js",
    "size-limit": [
        {
            "path": "dist/doggo.js.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/doggo.js.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^5.0.3",
        "husky": "^7.0.2",
        "size-limit": "^5.0.3",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.1",
        "typescript": "^4.4.2"
    },
    "dependencies": {
        "isomorphic-unfetch": "^3.1.0"
    },
    "keywords": [
        "doggo",
        "doggo.ninja",
        "api",
        "client",
        "typescript"
    ]
}
